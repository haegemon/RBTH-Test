<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}RBTH Test{% endblock %}</title>

    {% block styles %}
        <link href="{{ asset('bundles/RTBHTest/css/bootstrap.min.css') }}" rel="stylesheet">

        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    {% endblock %}

    {% block scriptes %}
        <script src="{{ asset('bundles/RTBHTest/js/jquery.js') }}"></script>
        <script src="{{ asset('bundles/RTBHTest/js/jquery.form.js') }}"></script>
        <script src="{{ asset('bundles/RTBHTest/js/bootstrap.min.js') }}"></script>
    {% endblock %}
</head>

<body>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span8 offset3" id="content">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>
</body>
</html>
